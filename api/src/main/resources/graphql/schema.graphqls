scalar Long

type Query {
    classroom(id: ID!): Classroom
}

type Mutation {
    createClassroom(params: ClassroomCreateParams!): Classroom
    deleteClassroom(id: ID!): BooleanResult
}

type BooleanResult {
    ok: Boolean!
}

type Classroom {
    id: ID!
    no: String!
    name: String!
    userNum: Long!
    createdAt: Long!
    updatedAt: Long!
}

input ClassroomCreateParams {
    name: String!
}